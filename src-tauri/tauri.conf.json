{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "nullstrap",
  "version": "0.1.0",
  "identifier": "com.nullstrap.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "splashscreen",
        "url": "splashscreen.html",
        "width": 600,
        "height": 350,
        "visible": true,
        "center": true,
        "maximizable": false,
        "decorations": false,
        "shadow": true,
        "title": "nullstrap"
      },
      {
        "label": "main",
        "url": "index.html",
        "width": 800,
        "height": 600,
        "visible": false,
        "title": "nullstrap",
        "decorations": false,
        "shadow": true,
        "transparent": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
